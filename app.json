{
    "runtime": {
        "arguments": "--v=1 --inspect",
        "version": "canary"
    },
    "shortcut": {
        "company": "OpenFin",
        "description": "Platform app seed local",
        "icon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "name": "Platform app seed local"
    },
    "appAssets": [
        {
            "src": "http://localhost:5555/my_platform_notes.zip",
            "alias": "my_platform_notes",
            "target": "my_platform_notes.txt",
            "version": "1.1.1"
        }
    ],
    "platform": {
        "uuid": "platform_customization_local",
        "applicationIcon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "autoShow": true,
        "providerUrl": "http://localhost:5555/provider.html",
        "permissions": {
            "ExternalWindow": {
              	"wrap": true
            },
            "System": {
                "getAllExternalWindows": true,
                "launchExternalProcess": true,
                "readRegistryValue": false,
                "terminateExternalProcess": true
            }
        },
        "defaultWindowOptions": {
            "url": "http://localhost:5555/platform-window.html",
            "contextMenu": true,
            "defaultWidth": 600,
            "defaultHeight": 600,
            "defaultLeft": 0,
            "defaultTop": 0,
            "saveWindowState": false,
            "showBackgroundImages": true,
            "backgroundThrottling": true,
            "minHeight": 445,
            "minWidth": 354,
            "permissions": {
                "System": {
                    "launchExternalProcess": true
                }
            }
        },
        "defaultViewOptions": {
            "experimental": {
                "childWindows": true
            }
        },
        "commands": [
            {
                "command": "stack.nextTab",
                "keys": "Ctrl+Tab"
            }
        ],
        "customData": {
            "nativeApplications": [
                {
                    "name": "Notepad",
                    "title": "my_platform_notes",
                    "launch": {
                        "_path": "C:\\Users\\rdepena\\Desktop\\my_platform_notes.txt",
                        "alias": "my_platform_notes",
                        "lifetime": "window",
                        "arguments": ""
                    }
                },
                {
                    "name": "excel",
                    "title": "",
                    "launch": {
                        "path": "excel",
                        "lifetime": "window",
                        "arguments": ""
                    }
                }
            ]
        }
    },
    "snapshot": {
        "windows": [
            {
                "layout": {
                    "content": [
                        {
                            "type": "stack",
                            "content": [
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "https://cdn.openfin.co/embed-web/chart.html"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    }
}
