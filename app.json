{
    "runtime": {
        "arguments": "--v=1 --inspect",
        "version": "31.112.75.7"
    },
    "shortcut": {
        "company": "OpenFin",
        "description": "Platform app seed local",
        "icon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "name": "Platform app seed local"
    },
    "platform": {
        "uuid": "platform_customization_local",
        "applicationIcon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "autoShow": false,
        "providerUrl": "http://localhost:5555/provider.html",
        "permissions": {
            "ExternalWindow": {
              	"wrap": true
            },
            "System": {
                "getAllExternalWindows": true,
                "launchExternalProcess": true,
                "readRegistryValue": false,
                "terminateExternalProcess": true
            }
        },
        "defaultWindowOptions": {
            "url": "http://localhost:5555/platform-window.html",
            "contextMenuSettings": {
                "reload": false,
                "devtools": true,
                "enable": true,
                "zoom": true
            },
            "accelerator": {
                "reload": true,
                "devtools": true,
                "reloadIgnoringCache":false,
                "zoom": true
            },
            "defaultWidth": 900,
            "defaultHeight": 720,
            "defaultLeft": 0,
            "defaultTop": 0,
            "saveWindowState": false,
            "backgroundThrottling": true,
            "minHeight": 445,
            "minWidth": 354,
            "experimental": {
                "analytics": true
            }
        },
        "defaultViewOptions": {
            "experimental": {
                "childWindows": true,
                "analytics": true
            },
            "accelerator": {
                "reload": true,
                "devtools": true,
                "reloadIgnoringCache":false,
                "zoom": true
            }
        },
        "commands": [
            {
                "command": "stack.nextTab",
                "keys": "Ctrl+Tab"
            }
        ],
        "viewVisibility": {
            "showViewsOnSplitterDrag": { "enabled": true },
            "showViewsOnWindowResize": { "enabled": true, "paintIntervalMs": 0 },
            "showViewsOnTabDrag": { "enabled": true }
        }
    },
    "snapshot": {
        "windows": [
            {
                "name": "Seed Window",
                "layout": {
                    "content": [
                        {
                            "type": "stack",
                            "id": "no-drop-target",
                            "content": [
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "https://cdn.openfin.co/health/deployment/index.html"
                                    }
                                },
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "https://google.com"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    }
}
