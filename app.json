{
    "runtime": {
        "arguments": "--v=1 --inspect",
        "version": "stable"
    },
    "shortcut": {
        "company": "OpenFin",
        "description": "Platform app seed local",
        "icon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "name": "Platform app seed local"
    },
    "appAssets": [
        {
            "src": "http://localhost:5562/my_platform_notes.zip",
            "alias": "my_platform_notes",
            "version": "1.1.6"
        }
    ],
    "platform": {
        "uuid": "platform_customization_local_native_window",
        "applicationIcon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "autoShow": true,
        "providerUrl": "http://localhost:5562/provider.html",
        "permissions": {
            "ExternalWindow": {
              	"wrap": true
            },
            "System": {
                "getAllExternalWindows": true,
                "launchExternalProcess": true,
                "readRegistryValue": false,
                "terminateExternalProcess": true
            }
        },
        "defaultWindowOptions": {
            "url": "http://localhost:5562/platform-window.html",
            "contextMenu": true,
            "defaultWidth": 600,
            "defaultHeight": 600,
            "defaultLeft": 0,
            "defaultTop": 0,
            "saveWindowState": false,
            "showBackgroundImages": true,
            "backgroundThrottling": true,
            "minHeight": 445,
            "minWidth": 354,
            "permissions": {
                "System": {
                    "launchExternalProcess": true
                }
            }
        },
        "defaultViewOptions": {
            "experimental": {
                "childWindows": true,
                "contentCreation": {
                    "rules": [
                        {
                            "behavior": "browser",
                            "match": [
                                "*://*.zoom.us/*",
                                "*://zoom.us/*",
                                "*://www.google.com/url?q=*",
                                "*://*.spotify.com/*",
                                "*://*spotify.com/*"
                            ]
                        },
                        {
                            "behavior": "view",
                            "match": [
                                "<all_urls>"
                            ]
                        }
                    ]
                }
            }
        },
        "commands": [
            {
                "command": "stack.nextTab",
                "keys": "Ctrl+Tab"
            }
        ],
        "customData": {
            "nativeApplications": [
                {
                    "name": "Notepad",
                    "title": "my_platform_notes",
                    "launch": {
                        "alias": "my_platform_notes",
                        "target": "my_platform_notes.txt",
                        "lifetime": "application",
                        "arguments": ""
                    }
                },
                {
                    "name": "Excel",
                    "title": "months-in-2020",
                    "launch": {
                        "alias": "my_platform_notes",
                        "target": "months-in-2020.xlsx",
                        "lifetime": "application",
                        "arguments": ""
                    }
                },
                {
                    "name": "MySimpleDotNetApp",
                    "title": "Form1",
                    "launch": {
                        "alias": "my_platform_notes",
                        "target": "MySimpleDotNetApp.exe",
                        "lifetime": "application",
                        "arguments": ""
                    }
                },
                {
                    "name": "firefox",
                    "title": "",
                    "launch": {
                        "target": "firefox.exe",
                        "lifetime": "application",
                        "arguments": ""
                    }
                }
            ]
        }
    },
    "snapshot": {
        "windows": [
            {
                "layout": {
                    "settings": {
                        "showPopoutIcon": true,
                        "showCloseIcon": true,
                        "showMaximiseIcon": true,
                        "popoutWholeStack": false
                    },
                    "content": [
                        {
                            "type": "stack",
                            "content": [
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "https://cdn.openfin.co/embed-web/chart.html"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    }
}
