{
    "runtime": {
        "arguments": "--v=1 --inspect --enable-mesh --autoplay-policy=no-user-gesture-required",
        "version": "22.94.66.4"
    },
    "shortcut": {
        "company": "OpenFin",
        "description": "bidfx_channel_issues",
        "icon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "name": "bidfx_channel_issues"
    },
    "appAssets" : [
        {
            "alias" : "dotnet-provider",
            "src" : "http://localhost:8080/bidfx_channel_issues.zip",
            "target" : "BidFXChannelIssues.exe",
            "version": "1.0.0"
        }
    ],
    "platform": {
        "uuid": "bidfx_channel_issues",
        "applicationIcon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "autoShow": true,
        "providerUrl": "http://localhost:5555/provider.html",
        "permissions": {
            "ExternalWindow": {
              	"wrap": true
            },
            "System": {
                "getAllExternalWindows": true,
                "launchExternalProcess": true,
                "readRegistryValue": false,
                "terminateExternalProcess": true
            }
        },
        "defaultWindowOptions": {
            "url": "http://localhost:5555/platform-window.html",
            "contextMenuSettings": {
                "reload": false,
                "devtools": false,
                "enable": false
            },
            "defaultWidth": 900,
            "defaultHeight": 720,
            "defaultLeft": 0,
            "defaultTop": 0,
            "saveWindowState": false,
            "backgroundThrottling": true,
            "minHeight": 445,
            "minWidth": 354,
            "experimental": {
                "analytics": true
            }
        },
        "defaultViewOptions": {
            "experimental": {
                "childWindows": true,
                "analytics": true
            }
        },
        "commands": [
            {
                "command": "stack.nextTab",
                "keys": "Ctrl+Tab"
            }
        ]
    },
    "snapshot": {
        "windows": [
            {
                "name": "Seed Window",
                "layout": {
                    "content": [
                        {
                            "type": "stack",
                            "id": "no-drop-target",
                            "content": [
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "https://cdn.openfin.co/health/deployment/index.html"
                                    }
                                },
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "https://google.com"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    }
}
