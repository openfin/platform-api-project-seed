{
    "runtime": {
        "arguments": "--v=1 --inspect",
        "version": "stable-v33"
    },
    "shortcut": {
        "company": "OpenFin",
        "description": "Platform app seed local",
        "icon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "name": "Platform app seed local"
    },
    
    "platform": {
        "uuid": "platform_customization_local",
        "applicationIcon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "autoShow": false,
        "providerUrl": "http://localhost:5555/provider.html",
        "defaultDomainSettings": {
            "rules": [
                {
                    "match": ["<all_urls>"],
                    "options": {
                        "downloadSettings": {
                            "rules": [
                                {
                                    "match": ["<all_urls>"],
                                    "behavior": "prompt"
                                }
                            ]
                        }
                    }
                }
            ]
        },
        "permissions": {
            "Application": {
                "getFileDownloadLocation": true,
                "setFileDownloadLocation": true
            },
            "ExternalWindow": {
              	"wrap": true
            },
            "System": {
                "getAllExternalWindows": true,
                "readRegistryValue": false,
                "terminateExternalProcess": true,
                "launchExternalProcess": {
                    "enabled": true,
                    "assets": {
                        "enabled": true
                    },
                    "downloads": {
                        "enabled": true
                    },
                    "executables": {
                        "enabled": true
                    }
                }
            }
        },
        "defaultWindowOptions": {
            "permissions": {
                "Application": {
                    "getFileDownloadLocation": true,
                    "setFileDownloadLocation": true
                },
                "System": {
                    "launchExternalProcess": {
                        "enabled": true,
                        "assets": {
                            "enabled": true
                        },
                        "downloads": {
                            "enabled": true
                        },
                        "executables": {
                            "enabled": true
                        }
                    },
                    "readRegistryValue": false,
                    "terminateExternalProcess": true
                }
            },
            "url": "http://localhost:5555/platform-window.html",
            "contextMenuSettings": {
                "reload": false,
                "devtools": true,
                "enable": true,
                "zoom": true
            },
            "accelerator": {
                "reload": true,
                "devtools": true,
                "reloadIgnoringCache":false,
                "zoom": true
            },
            "defaultWidth": 900,
            "defaultHeight": 720,
            "defaultLeft": 0,
            "defaultTop": 0,
            "saveWindowState": false,
            "backgroundThrottling": true,
            "minHeight": 445,
            "minWidth": 354,
            "experimental": {
                "analytics": true
            }
        },
        "defaultViewOptions": {
            "permissions": {
                "Application": {
                    "getFileDownloadLocation": true,
                    "setFileDownloadLocation": true
                },
                "System": {
                    "launchExternalProcess": {
                        "enabled": true,
                        "assets": {
                            "enabled": true
                        },
                        "downloads": {
                            "enabled": true
                        },
                        "executables": {
                            "enabled": true
                        }
                    },
                    "readRegistryValue": false,
                    "terminateExternalProcess": true
                }
            },
            "experimental": {
                "childWindows": true,
                "analytics": true
            },
            "accelerator": {
                "reload": true,
                "devtools": true,
                "reloadIgnoringCache":false,
                "zoom": true
            },
            "spellCheck":true,
            "contextMenuOptions": {
                "enabled": true,
                "template": [
                    "spellCheck",
                    "separator",
                    "undo",
                    "redo",
                    "cut",
                    "separator",
                    "copy",
                    "paste",
                    "selectAll",
                    "inspect",
                    "reload",
                    "navigateForward",
                    "navigateBack",
                    "print",
                    "copyImage"
                   ]
        }
        },
        "commands": [
            {
                "command": "stack.nextTab",
                "keys": "Ctrl+Tab"
            }
        ],
        "viewVisibility": {
            "showViewsOnSplitterDrag": { "enabled": true },
            "showViewsOnWindowResize": { "enabled": true, "paintIntervalMs": 0 },
            "showViewsOnTabDrag": { "enabled": true }
        }
    },
    "snapshot": {
        "windows": [
            {
                "name": "Seed Window",
                "layout": {
                    "content": [
                        {
                            "type": "stack",
                            "id": "no-drop-target",
                            "content": [
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "https://cdn.openfin.co/health/deployment/index.html"
                                    }
                                },
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "https://google.com"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    },
    "appAssets": [
        {
          "src": "http://localhost:5555/assets/assetTest.zip",
          "alias": "cwdTest",
          "target": "testCWD.bat",
          "version": "1.0.0"
        },
        {
            "alias": "winform-interop-example",
            "version": "4.0.0",
            "src": "https://built-on-openfin.github.io/workspace-starter/workspace/v8.0.0/common/assets/native-interop-example.zip",
            "target": "OpenFin.Interop.Win.Sample.exe",
            "forceDownload": true
        }
    ]
}
